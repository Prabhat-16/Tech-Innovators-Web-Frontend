# Example Environment Configuration
# Copy this file to .env and fill in your actual values

# Environment Configuration
NODE_ENV=development

# App Configuration
VITE_APP_NAME="MIT Tech Innovators Club"
VITE_APP_VERSION=1.0.0

# API Configuration (if you have any APIs)
# VITE_API_URL=https://your-api-url.com
# VITE_API_KEY=your-api-key-here

# Database Configuration (if applicable)
# DATABASE_URL=your-database-url-here

# Third-party Services (examples)
# VITE_GOOGLE_ANALYTICS_ID=your-ga-id
# VITE_FIREBASE_CONFIG=your-firebase-config
# VITE_SUPABASE_URL=your-supabase-url
# VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Social Media Links
VITE_GITHUB_URL=https://github.com/your-org
VITE_TWITTER_URL=https://twitter.com/your-handle
VITE_LINKEDIN_URL=https://linkedin.com/company/your-company

# Contact Information
VITE_CONTACT_EMAIL=hello@mittechinnovators.org
VITE_CONTACT_PHONE=(617) 555-0123
VITE_CONTACT_ADDRESS="MIT Campus, Cambridge, MA"

# Theme Configuration
VITE_DEFAULT_THEME=theme-blue-purple
VITE_ENABLE_THEME_SWITCHER=true
